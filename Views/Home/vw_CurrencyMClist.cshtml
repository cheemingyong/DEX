		if (vw_CurrencyMC_list.RowAction != "delete" && vw_CurrencyMC_list.RowAction != "insertdelete" && !(vw_CurrencyMC_list.RowAction == "insert" && vw_CurrencyMC_list.CurrentAction == "F" && vw_CurrencyMC_list.EmptyRow())) {
