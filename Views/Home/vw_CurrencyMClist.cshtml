@if (ew_Empty(vw_CurrencyMC.Export)) {
<script type="text/javascript">

// Form object
var CurrentPageID = EW_PAGE_ID = "list";
var CurrentForm = fvw_CurrencyMClist = new ew_Form("fvw_CurrencyMClist", "list");
fvw_CurrencyMClist.FormKeyCountName = "@vw_CurrencyMC_list.FormKeyCountName";

// Validate form
fvw_CurrencyMClist.Validate = function() {
	if (!this.ValidateRequired)
		return true; // Ignore validation
	var $ = jQuery, fobj = this.GetForm(), $fobj = $(fobj);
	if ($fobj.find("#a_confirm").val() == "F")
		return true;
	var elm, felm, uelm, addcnt = 0;
	var $k = $fobj.find("#" + this.FormKeyCountName); // Get key_count
	var rowcnt = ($k[0]) ? parseInt($k.val(), 10) : 1;
	var startcnt = (rowcnt == 0) ? 0 : 1; // Check rowcnt == 0 => Inline-Add
	var gridinsert = $fobj.find("#a_list").val() == "gridinsert";
	for (var i = startcnt; i <= rowcnt; i++) {
		var infix = ($k[0]) ? String(i) : "";
		$fobj.data("rowindex", infix);
		var checkrow = (gridinsert) ? !this.EmptyRow(infix) : true;
		if (checkrow) {
			addcnt++;
		elm = this.GetElements("x" + infix + "_CurrencyCode");
		if (elm && !ew_IsHidden(elm) && !ew_HasValue(elm))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.CurrencyCode.ReqErrMsg.Replace("%s", vw_CurrencyMC.CurrencyCode.FldCaption)))");
		elm = this.GetElements("x" + infix + "_MCBuyRate");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCBuyRate.FldErrMsg))");
		elm = this.GetElements("x" + infix + "_MCBuyMinBid");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCBuyMinBid.FldErrMsg))");
		elm = this.GetElements("x" + infix + "_MCBuyMaxBid");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCBuyMaxBid.FldErrMsg))");
		elm = this.GetElements("x" + infix + "_MCSellRate");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCSellRate.FldErrMsg))");
		elm = this.GetElements("x" + infix + "_MCSellMinBid");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCSellMinBid.FldErrMsg))");
		elm = this.GetElements("x" + infix + "_MCSellMaxBid");
		if (elm && !ew_CheckNumber(elm.value))
			return this.OnError(elm, "@Html.Raw(ew_JsEncode2(vw_CurrencyMC.MCSellMaxBid.FldErrMsg))");

		// Fire Form_CustomValidate event
		if (!this.Form_CustomValidate(fobj))
			return false;
		} // End Grid Add checking
	}
	if (gridinsert && addcnt == 0) { // No row added
		ew_Alert(ewLanguage.Phrase("NoAddRecord"));
		return false;
	}
	return true;
}

// Check empty row
fvw_CurrencyMClist.EmptyRow = function(infix) {
	var fobj = this.Form;
	if (ew_ValueChanged(fobj, infix, "CurrencyCode", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCBuyRate", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCBuyRateType", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCBuyMinBid", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCBuyMaxBid", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCSellRate", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCSellRateType", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCSellMinBid", false)) return false;
	if (ew_ValueChanged(fobj, infix, "MCSellMaxBid", false)) return false;
	return true;
}

// Form_CustomValidate event
fvw_CurrencyMClist.Form_CustomValidate = 
 function(fobj) { // DO NOT CHANGE THIS LINE!

 	// Your custom validation code here, return false if invalid.
 	return true;
 }

// Use JavaScript validation or not remove PROJ.ProgramLanguage
fvw_CurrencyMClist.ValidateRequired = @((EW_CLIENT_VALIDATE) ? "true" : "false");

// Dynamic selection lists
fvw_CurrencyMClist.Lists["x_MCBuyRateType"] = {"LinkField":"","Ajax":null,"AutoFill":false,"DisplayFields":["","","",""],"ParentFields":[],"ChildFields":[],"FilterFields":[],"Options":[],"Template":""};
