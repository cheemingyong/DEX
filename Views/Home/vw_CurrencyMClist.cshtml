fvw_CurrencyMClist.Lists["x_MCSellRateType"] = {"LinkField":"","Ajax":null,"AutoFill":false,"DisplayFields":["","","",""],"ParentFields":[],"ChildFields":[],"FilterFields":[],"Options":[],"Template":""};
fvw_CurrencyMClist.Lists["x_MCSellRateType"].Options = @Html.Raw(ew_ArrayToJson(vw_CurrencyMC.MCSellRateType.Options()));

// Form object for search
var CurrentSearchForm = fvw_CurrencyMClistsrch = new ew_Form("fvw_CurrencyMClistsrch");

// Init search panel as collapsed
if (fvw_CurrencyMClistsrch) fvw_CurrencyMClistsrch.InitSearchPanel = true;
