@{ // Begin Code
if (vw_CurrencyMC_list.ExportAll && ew_NotEmpty(vw_CurrencyMC_list.Export)) {
	vw_CurrencyMC_list.StopRec = vw_CurrencyMC_list.TotalRecs;
} else {

	// Set the last record to display
	if (vw_CurrencyMC_list.TotalRecs > vw_CurrencyMC_list.StartRec + vw_CurrencyMC_list.DisplayRecs - 1) {
		vw_CurrencyMC_list.StopRec = vw_CurrencyMC_list.StartRec + vw_CurrencyMC_list.DisplayRecs - 1;
	} else {
		vw_CurrencyMC_list.StopRec = vw_CurrencyMC_list.TotalRecs;
	}
}

// Restore number of post back records
if (ObjForm != null) {
	ObjForm.Index = -1;
	if (ObjForm.HasValue(vw_CurrencyMC_list.FormKeyCountName) && (vw_CurrencyMC_list.CurrentAction == "gridadd" || vw_CurrencyMC_list.CurrentAction == "gridedit" || vw_CurrencyMC_list.CurrentAction == "F")) {
		vw_CurrencyMC_list.KeyCount = ew_ConvertToInt(ObjForm.GetValue(vw_CurrencyMC_list.FormKeyCountName));
		vw_CurrencyMC_list.StopRec = vw_CurrencyMC_list.StartRec + vw_CurrencyMC_list.KeyCount - 1;
	}
}
if (vw_CurrencyMC_list.Recordset != null && vw_CurrencyMC_list.Recordset.HasRows) {
	if (!vw_CurrencyMC_list.Connection.SelectOffset) { // DN
		for (var i = 1; i <= vw_CurrencyMC_list.StartRec - 1; i++) { // Move to first record
			if (vw_CurrencyMC_list.Recordset.Read()) {
				vw_CurrencyMC_list.RecCnt++;
			}
		}
	} else {
		vw_CurrencyMC_list.RecCnt = vw_CurrencyMC_list.StartRec - 1;
	}
} else if (!vw_CurrencyMC_list.AllowAddDeleteRow && vw_CurrencyMC_list.StopRec == 0) {
	vw_CurrencyMC_list.StopRec = vw_CurrencyMC_list.GridAddRowCount;
}

// Initialize aggregate
vw_CurrencyMC_list.RowType = EW_ROWTYPE_AGGREGATEINIT;
vw_CurrencyMC_list.ResetAttrs();
vw_CurrencyMC_list.RenderRow();
if (vw_CurrencyMC_list.CurrentAction == "gridadd") {
	vw_CurrencyMC_list.RowIndex = 0;
}
if (vw_CurrencyMC_list.CurrentAction == "gridedit") {
	vw_CurrencyMC_list.RowIndex = 0;
}

// Output data rows
bool Eof = (vw_CurrencyMC_list.Recordset == null); // DN
while (vw_CurrencyMC_list.RecCnt < vw_CurrencyMC_list.StopRec) { // Begin While
	if (vw_CurrencyMC_list.CurrentAction != "gridadd" && !Eof) { // DN
		Eof = !vw_CurrencyMC_list.Recordset.Read();
	}
	vw_CurrencyMC_list.RecCnt++;
	if (vw_CurrencyMC_list.RecCnt >= vw_CurrencyMC_list.StartRec) {
		vw_CurrencyMC_list.RowCnt++;
		if (vw_CurrencyMC_list.CurrentAction == "gridadd" || vw_CurrencyMC_list.CurrentAction == "gridedit" || vw_CurrencyMC_list.CurrentAction == "F") {
			vw_CurrencyMC_list.RowIndex = ew_ConvertToInt(vw_CurrencyMC_list.RowIndex) + 1;
			ObjForm.Index = ew_ConvertToInt(vw_CurrencyMC_list.RowIndex);
			if (ObjForm.HasValue(vw_CurrencyMC_list.FormActionName)) {
				vw_CurrencyMC_list.RowAction = ObjForm.GetValue(vw_CurrencyMC_list.FormActionName);
			} else if (vw_CurrencyMC_list.CurrentAction == "gridadd") {
				vw_CurrencyMC_list.RowAction = "insert";
			} else {
				vw_CurrencyMC_list.RowAction = "";
			}
		}

		// Set up key count
		vw_CurrencyMC_list.KeyCount = ew_ConvertToInt(vw_CurrencyMC_list.RowIndex);

		// Init row class and style
		vw_CurrencyMC_list.ResetAttrs();
		vw_CurrencyMC_list.CssClass = "";
		if (vw_CurrencyMC_list.CurrentAction == "gridadd") {
			vw_CurrencyMC_list.LoadDefaultValues(); // Load default values
		} else {
			vw_CurrencyMC_list.LoadRowValues(vw_CurrencyMC_list.Recordset); // Load row values
		}
		vw_CurrencyMC_list.RowType = EW_ROWTYPE_VIEW; // Render view
		if (vw_CurrencyMC_list.CurrentAction == "gridadd") { // Grid add
			vw_CurrencyMC_list.RowType = EW_ROWTYPE_ADD; // Render add
		}
		if (vw_CurrencyMC_list.CurrentAction == "gridadd" && vw_CurrencyMC_list.EventCancelled && !ObjForm.HasValue("k_blankrow")) { // Insert failed
			vw_CurrencyMC_list.RestoreCurrentRowFormValues(vw_CurrencyMC_list.RowIndex); // Restore form values
		}
		if (vw_CurrencyMC_list.CurrentAction == "gridedit") { // Grid edit
			if (vw_CurrencyMC_list.EventCancelled) {
				vw_CurrencyMC_list.RestoreCurrentRowFormValues(vw_CurrencyMC_list.RowIndex); // Restore form values
			}
			if (vw_CurrencyMC_list.RowAction == "insert") {
				vw_CurrencyMC_list.RowType = EW_ROWTYPE_ADD; // Render add
			} else {
				vw_CurrencyMC_list.RowType = EW_ROWTYPE_EDIT; // Render edit
			}
		}
		if (vw_CurrencyMC_list.CurrentAction == "gridedit" && (vw_CurrencyMC_list.RowType == EW_ROWTYPE_EDIT || vw_CurrencyMC_list.RowType == EW_ROWTYPE_ADD) && vw_CurrencyMC_list.EventCancelled) { // Update failed
			vw_CurrencyMC_list.RestoreCurrentRowFormValues(vw_CurrencyMC_list.RowIndex); // Restore form values
		}
		if (vw_CurrencyMC_list.RowType == EW_ROWTYPE_EDIT) { // Edit row
			vw_CurrencyMC_list.EditRowCnt++;
		}

		// Set up row id / data-rowindex
		vw_CurrencyMC_list.RowAttrs.Add("data-rowindex", Convert.ToString(vw_CurrencyMC_list.RowCnt));
