			vw_CurrencyMC_list.SetUpStartRec(); // Set up start record position
		}
		vw_CurrencyMC_list.Recordset = vw_CurrencyMC_list.LoadRecordset(vw_CurrencyMC_list.StartRec - 1, vw_CurrencyMC_list.DisplayRecs);

		// Set no record found message
		if (vw_CurrencyMC_list.CurrentAction == "" && vw_CurrencyMC_list.TotalRecs == 0) {
			if (vw_CurrencyMC_list.SearchWhere == "0=101") {
