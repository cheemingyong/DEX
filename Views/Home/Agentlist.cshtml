<tbody>
@{ // Begin Code
if (Agent_list.ExportAll && ew_NotEmpty(Agent_list.Export)) {
	Agent_list.StopRec = Agent_list.TotalRecs;
} else {

	// Set the last record to display
	if (Agent_list.TotalRecs > Agent_list.StartRec + Agent_list.DisplayRecs - 1) {
		Agent_list.StopRec = Agent_list.StartRec + Agent_list.DisplayRecs - 1;
	} else {
		Agent_list.StopRec = Agent_list.TotalRecs;
	}
}

// Restore number of post back records
if (ObjForm != null) {
	ObjForm.Index = -1;
	if (ObjForm.HasValue(Agent_list.FormKeyCountName) && (Agent_list.CurrentAction == "gridadd" || Agent_list.CurrentAction == "gridedit" || Agent_list.CurrentAction == "F")) {
		Agent_list.KeyCount = ew_ConvertToInt(ObjForm.GetValue(Agent_list.FormKeyCountName));
		Agent_list.StopRec = Agent_list.StartRec + Agent_list.KeyCount - 1;
	}
}
if (Agent_list.Recordset != null && Agent_list.Recordset.HasRows) {
	if (!Agent_list.Connection.SelectOffset) { // DN
		for (var i = 1; i <= Agent_list.StartRec - 1; i++) { // Move to first record
			if (Agent_list.Recordset.Read()) {
				Agent_list.RecCnt++;
			}
		}
	} else {
		Agent_list.RecCnt = Agent_list.StartRec - 1;
	}
} else if (!Agent_list.AllowAddDeleteRow && Agent_list.StopRec == 0) {
	Agent_list.StopRec = Agent_list.GridAddRowCount;
}

// Initialize aggregate
Agent_list.RowType = EW_ROWTYPE_AGGREGATEINIT;
Agent_list.ResetAttrs();
Agent_list.RenderRow();
if (Agent_list.CurrentAction == "gridadd") {
	Agent_list.RowIndex = 0;
}
if (Agent_list.CurrentAction == "gridedit") {
	Agent_list.RowIndex = 0;
}

// Output data rows
bool Eof = (Agent_list.Recordset == null); // DN
while (Agent_list.RecCnt < Agent_list.StopRec) { // Begin While
	if (Agent_list.CurrentAction != "gridadd" && !Eof) { // DN
		Eof = !Agent_list.Recordset.Read();
	}
	Agent_list.RecCnt++;
	if (Agent_list.RecCnt >= Agent_list.StartRec) {
		Agent_list.RowCnt++;
		if (Agent_list.CurrentAction == "gridadd" || Agent_list.CurrentAction == "gridedit" || Agent_list.CurrentAction == "F") {
			Agent_list.RowIndex = ew_ConvertToInt(Agent_list.RowIndex) + 1;
			ObjForm.Index = ew_ConvertToInt(Agent_list.RowIndex);
			if (ObjForm.HasValue(Agent_list.FormActionName)) {
				Agent_list.RowAction = ObjForm.GetValue(Agent_list.FormActionName);
			} else if (Agent_list.CurrentAction == "gridadd") {
				Agent_list.RowAction = "insert";
			} else {
				Agent_list.RowAction = "";
			}
		}

		// Set up key count
		Agent_list.KeyCount = ew_ConvertToInt(Agent_list.RowIndex);

		// Init row class and style
		Agent_list.ResetAttrs();
		Agent_list.CssClass = "";
		if (Agent_list.CurrentAction == "gridadd") {
			Agent_list.LoadDefaultValues(); // Load default values
		} else {
			Agent_list.LoadRowValues(Agent_list.Recordset); // Load row values
		}
		Agent_list.RowType = EW_ROWTYPE_VIEW; // Render view
		if (Agent_list.CurrentAction == "gridadd") { // Grid add
			Agent_list.RowType = EW_ROWTYPE_ADD; // Render add
		}
		if (Agent_list.CurrentAction == "gridadd" && Agent_list.EventCancelled && !ObjForm.HasValue("k_blankrow")) { // Insert failed
			Agent_list.RestoreCurrentRowFormValues(Agent_list.RowIndex); // Restore form values
		}
		if (Agent_list.CurrentAction == "gridedit") { // Grid edit
			if (Agent_list.EventCancelled) {
				Agent_list.RestoreCurrentRowFormValues(Agent_list.RowIndex); // Restore form values
			}
			if (Agent_list.RowAction == "insert") {
				Agent_list.RowType = EW_ROWTYPE_ADD; // Render add
			} else {
				Agent_list.RowType = EW_ROWTYPE_EDIT; // Render edit
			}
		}
		if (Agent_list.CurrentAction == "gridedit" && (Agent_list.RowType == EW_ROWTYPE_EDIT || Agent_list.RowType == EW_ROWTYPE_ADD) && Agent_list.EventCancelled) { // Update failed
			Agent_list.RestoreCurrentRowFormValues(Agent_list.RowIndex); // Restore form values
		}
		if (Agent_list.RowType == EW_ROWTYPE_EDIT) { // Edit row
			Agent_list.EditRowCnt++;
		}

		// Set up row id / data-rowindex
		Agent_list.RowAttrs.Add("data-rowindex", Convert.ToString(Agent_list.RowCnt));
		Agent_list.RowAttrs.Add("id", "r" + Convert.ToString(Agent_list.RowCnt) + "_Agent");
		Agent_list.RowAttrs.Add("data-rowtype", Convert.ToString(Agent_list.RowType));

		// Render row
		Agent_list.RenderRow();

		// Render list options
