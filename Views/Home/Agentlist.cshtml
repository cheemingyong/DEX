@{
	if (Agent_list.CurrentAction == "gridadd") {
		Agent_list.CurrentFilter = "0=1";
		Agent_list.StartRec = 1;
		Agent_list.DisplayRecs = Agent_list.GridAddRowCount;
		Agent_list.TotalRecs = Agent_list.DisplayRecs;
		Agent_list.StopRec = Agent_list.DisplayRecs;
	} else {
		if (Agent_list.TotalRecs <= 0) {
			Agent_list.TotalRecs = Agent_list.SelectRecordCount();
		}
		Agent_list.StartRec = 1;
		if (Agent_list.DisplayRecs <= 0 || ew_NotEmpty(Agent_list.Export) && Agent_list.ExportAll) { // Display all records
			Agent_list.DisplayRecs = Agent_list.TotalRecs;
		}
		if (!(ew_NotEmpty(Agent_list.Export) && Agent_list.ExportAll)) {
			Agent_list.SetUpStartRec(); // Set up start record position
		}
		Agent_list.Recordset = Agent_list.LoadRecordset(Agent_list.StartRec - 1, Agent_list.DisplayRecs);

		// Set no record found message
		if (Agent_list.CurrentAction == "" && Agent_list.TotalRecs == 0) {
			if (Agent_list.SearchWhere == "0=101") {
				Agent_list.WarningMessage = Language.Phrase("EnterSearchCriteria");
			} else {
				Agent_list.WarningMessage = Language.Phrase("NoRecord");
			}
		}
	}
	Agent_list.RenderOtherOptions();
}
